@model Proyecto_Relampago.Models.EjeEstrategico

@{
    ViewBag.Title = "Crear Eje Estratégico";
}

<h2>@ViewBag.Title</h2>

@using (Html.BeginForm())
{
    <div class="form-horizontal">
        <h4>Eje Estratégico</h4>
        <hr />
        <div class="form-group">
            @Html.LabelFor(model => model.idEje, htmlAttributes: new { @class = "control-label col-md-2" })
            <div class="col-md-10">
                @Html.TextBoxFor(model => model.idEje, new { @class = "form-control" })
                @Html.ValidationMessageFor(model => model.idEje, "", new { @class = "text-danger" })
            </div>
        </div>
        <div class="form-group">
            @Html.LabelFor(model => model.nombreEjeEstrategico, htmlAttributes: new { @class = "control-label col-md-2" })
            <div class="col-md-10">
                @Html.TextBoxFor(model => model.nombreEjeEstrategico, new { @class = "form-control" })
                @Html.ValidationMessageFor(model => model.nombreEjeEstrategico, "", new { @class = "text-danger" })
            </div>
        </div>

        <div class="form-group">
            <div class="col-md-offset-2 col-md-10">
                <button type="submit" class="btn btn-primary">Crear</button>
            </div>
        </div>
    </div>
}

<p>
    @Html.ActionLink("Volver a la lista", "Estrategia")
</p>
